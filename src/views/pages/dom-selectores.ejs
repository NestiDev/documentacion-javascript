                                <!--  
                                    tag-ul: en esta definimos los items del temario del modal
                                -->
                                <ul>
                                    <li><a href="#selector-tag">01. Selector tag</a></li>
                                    <li><a href="#selector-class">02. Selector class</a></li>
                                    <li><a href="#selector-id">03. Selector ID</a></li>
                                    <li><a href="#selector-hermanos">04. Selector hermanos</a></li>
                                    <li><a href="#selector-hijo">05. Selector hijo</a></li>
                                    <li><a href="#selector-hijos">06. Selector hijos</a></li>
                                    <li><a href="#selector-atributo">07. Selector atributo</a></li>
                                    <li><a href="#selector-por-atributo-relacionado">08. Selector atributo relacionado</a></li>
                                    <li><a href="#selector-data-">09. Selector data-</a></li>
                                    <li><a href="#selector->">10. Selector ></a></li>
                                    <li><a href="#selector-last-child">11. Selector last-child</a></li>
                                    <li><a href="#selector-nth-child(even)">12. Selector nth-child(even)</a></li>
                                    <li><a href="#selector-nth-child(odd)">13. Selector nth-child(odd)</a></li>
                                </ul>
                            </section>
                        </div>
                    </div>
                </div>

                <!--  
                    sections_container: contenedor de las secciones que componen
                    esta vista
                -->
