                            <pre><code><span class="index outer_index">10</span><span class="console"><</span><span class="parameters">h1 </span><span class="functions">id</span><span class="console">=</span><span class="string">"tag-h1"</span><span class="console">></span>Hola mundo<span class="console"><</span><span class="parameters">/h1</span><span class="console">></span></code></pre>
                            <pre><code><span class="index">11</span></code></pre>
                            <pre><code><span class="index outer_index">12</span><span class="console"><</span><span class="parameters">script </span><span class="functions">type</span><span class="console">=</span><span class="string">"module" </span><span class="functions">src</span><span class="console">=</span><span class="string">"./main.js"</span><span class="console">><</span><span class="parameters">/script</span><span class="console">></span></code></pre>
                            <pre><code><span class="index inner_index">13</span><span class="console"><</span><span class="parameters">/body</span><span class="console">></span></code></pre>
