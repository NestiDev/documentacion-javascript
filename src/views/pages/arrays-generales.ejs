                            <pre><code><span class="index inner_index">01</span><span class="variable">let </span>vocalesAnidadas <span class="operator">= </span><span class="square">[</span><span class="string">"a"</span><span class="semi_colon">, </span><span class="keyword">[</span><span class="string">"e"</span><span class="semi_colon">, </span><span class="parenthesis">[</span><span class="string">"i"</span><span class="semi_colon">, </span><span class="square">[</span><span class="string">"o"</span><span class="semi_colon">, </span><span class="keyword">[</span><span class="string">"u"</span><span class="keyword">]</span><span class="square">]</span><span class="parenthesis">]</span><span class="keyword">]</span><span class="square">]</span><span class="semi_colon">;</span></code></pre>

