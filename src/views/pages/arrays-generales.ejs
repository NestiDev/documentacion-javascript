                    <section class="sect_content" id="flat">
                        <h3>17. Flat</h3>

                        <p>texto pendiente</p>

                        <div class="code_box">
                            <pre><code><span class="circle first_circle"></span><span class="circle second_circle"></span><span class="circle thirt_circle"></span></code></pre>

                            <pre><code><span class="index inner_index">01</span><span class="variable">let </span>anidacion <span class="operator">= </span><span class="square">[</span><span class="string">"a"</span><span class="semi_colon">, </span><span class="string">"b"</span><span class="semi_colon">, </span><span class="keyword">[</span><span class="number">1250</span><span class="semi_colon">, </span><span class="number">1158</span><span class="keyword">]</span><span class="semi_colon">, </span><span class="string">"c"</span><span class="semi_colon">, </span><span class="keyword">[</span><span class="boolean">true</span><span class="semi_colon">, </span><span class="boolean">false</span><span class="keyword">]</span><span class="square">]</span><span class="semi_colon">;</span></code></pre>

